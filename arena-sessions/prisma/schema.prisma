generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Characters {
  id    String @id(map: "characters_pkey") @unique(map: "Character_id_key")
  name  String
  class String
  spec  String
}

model Matches {
  id     String   @id(map: "matches_pkey") @unique
  charId String
  isWin  Boolean?
  arena  String
  team   String
  enemy  String
}
